{
	"name": "hello",
	"displayName": "hello",
	"description": "hello",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hello.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hello.helloWorld",
				"title": "Hello World"
			}
		],
		"iconThemes": [
			{
				"id": "casbin",
				"label": "Casbin",
				"path": "./fileicons/casbin-icon-theme.json"
			}
		],
		"languages": [
            {
                "id": "casbin",
                "extensions": [".conf",".csv"]
            }
        ],
        "grammars": [
            {
                "language": "casbin",
                "scopeName": "source.conf",
                "path": "./syntaxes/casbin.grammar.json"
            }
        ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
